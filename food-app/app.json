{
  "expo": {
    "name": "GlucoBites",
    "slug": "GlucoBites",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/GlucoBitesLogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/GlucoBitesLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/GlucoBitesLogo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [],
      "package": "com.dope9402.GlucoBites",
      
      "networkSecurityConfig": {
      "domainConfigurations": {
        "api.glucobites.org": {
          "domains": ["13.214.71.4", "api.glucobites.org"],
          "includeSubdomains": false,
          "cleartextTrafficPermitted": true
        }
      }
}
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos",
          "cameraPermission": "Allow $(PRODUCT_NAME) to use your camera"
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "4ea78716-6ca7-47a9-8638-34b6f273b00f"
      }
    }
  }
}